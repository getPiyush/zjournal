{
  "name": "zjoutnal-react-ts",
  "version": "1.0.0",
  "description": "zJournal - A React TS bases journal editor application",
  "keywords": [
    "typescript",
    "react",
    "starter"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "axios": "^0.26.0",
    "crypto-js": "^4.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-font-loader": "^1.1.0",
    "react-html-parser": "^2.0.2",
    "react-phone-input-2": "^2.15.0",
    "react-router-dom": "6.2.1",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.3",
    "@types/jest": "^27.4.0",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "concurrently": "^7.0.0",
    "express": "^4.17.3",
    "json-server": "^0.17.0",
    "random-sentence": "^1.0.4",
    "typescript": "4.4.2"
  },
  "scripts": {
    "start": "set PORT=80&&react-scripts start",
    "prodrun": "cd build&&node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server:dev": "cd server/node&&node server.js -w --development",
    "server": "cd server/node&&node server.js -w --production",
    "dev": "concurrently \"yarn start\" \"yarn server:dev\"",
    "prod": "concurrently \"yarn prodrun\" \"yarn server\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
